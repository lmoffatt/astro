####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/opt/Astro/bin || mkdir -p $(INSTALL_ROOT)/opt/Astro/bin
	-$(INSTALL_PROGRAM) $(QMAKE_TARGET) $(INSTALL_ROOT)/opt/Astro/bin/$(QMAKE_TARGET)
	-$(STRIP) $(INSTALL_ROOT)/opt/Astro/bin/$(QMAKE_TARGET)

uninstall_target: FORCE
	-$(DEL_FILE) $(INSTALL_ROOT)/opt/Astro/bin/$(QMAKE_TARGET)
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/Astro/bin/ 


install: install_target  FORCE

uninstall: uninstall_target  FORCE

